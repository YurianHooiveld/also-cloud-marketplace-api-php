{
    "name": "inserve/also-cloud-marketplace-api-php",
    "description": "A PHP wrapper ALSO Cloud Marketplace",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/serializer": "^5.4.45",
        "symfony/property-access": "^5.4.45",
        "psr/log": "^1.1.4",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.41",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^2",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Inserve\\ALSOCloudMarketplaceAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inserve\\ALSOCloudMarketplaceAPI\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Inserve",
            "email": "dev@inserve.nl"
        }
    ],
    "minimum-stability": "stable"
}
